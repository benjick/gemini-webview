{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Gemini",
  "version": "0.1.0",
  "identifier": "com.max.gemini-webview",
  "build": {
    "beforeDevCommand": null,
    "devUrl": "https://gemini.google.com/",
    "beforeBuildCommand": null
  },
  "app": {
    "windows": [
      {
        "title": "Gemini",
        "width": 1200,
        "height": 800,
        "url": "https://gemini.google.com/",
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
